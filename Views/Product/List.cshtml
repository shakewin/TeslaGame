@model ProductListViewModel

    <div class="products">
    @foreach (var p in Model.Products)
    {
        @Html.Partial("ProductSummary", p)
    }
    </div>
    <div class="nav" page-model="@Model.PagingInfo" page-action="List"
         page-classes-enabled="true" page-class="btn" page-class-normal="btn-unselect"
         page-class-selected="btn-select">
    </div>
